AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"

# Domain for Traefik routing (if deployed to production)
DOMAIN=localhost

# PostgreSQL Configuration
POSTGRES_DB=semblance_db
POSTGRES_USER=semblance_user
